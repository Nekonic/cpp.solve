{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "${fileDirname}",
      "program": "${workspaceFolder}/${fileBasenameNoExtension}",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description":  "디스어셈블리 버전을 AT&T(으)로 설정",
          "text": "-gdb-set disassembly-flavor att",
          "ignoreFailures": true
        }
      ]
    }
  ]
}